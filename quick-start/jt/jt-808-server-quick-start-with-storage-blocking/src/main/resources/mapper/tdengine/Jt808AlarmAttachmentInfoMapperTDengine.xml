<mapper namespace="io.github.hylexus.xtream.quickstart.ext.jt808.withstorage.blocking.mapper.tdengine.Jt808AlarmAttachmentInfoMapperTDengine">
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from jt_808_alarm_attachment_info
    </select>
    <select id="list"
            resultType="io.github.hylexus.xtream.quickstart.ext.jt808.withstorage.blocking.domain.entity.Jt808AlarmAttachmentInfoEntity">
        select *
        from jt_808_alarm_attachment_info
        order by alarm_time desc
        limit #{dto.pageSize}
            offset #{dto.offset}
    </select>
</mapper>
